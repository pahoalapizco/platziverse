{
  "name": "platziverse-web",
  "version": "1.0.0",
  "description": "A simple dashboard for an IoT platform",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "prestart": "npm run build",
    "start-dev": "DEBUG=platziverse:* nodemon server.js",
    "prestart-dev": "npm run build",
    "build": "browserify -t babelify -t vueify client/app.js -o public/bundle.js",
    "lint": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pahoalapizco/platziverse.git"
  },
  "author": "pahoalapizco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pahoalapizco/platziverse/issues"
  },
  "homepage": "https://github.com/pahoalapizco/platziverse#readme",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babelify": "7.3.0",
    "browserify": "14.4.0",
    "nodemon": "^1.19.4",
    "standard": "^14.3.1",
    "vueify": "9.4.1"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "chart.js": "2.6.0",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "express-asyncify": "^1.0.0",
    "platziverse-utils": "file:../platziverse-utils",
    "platziverse-agent": "file:../platziverse-agent",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "socket.io": "^2.3.0",
    "vue": "2.4.2",
    "vue-chartjs": "2.8.1",
    "vue-template-compiler": "^2.4.2"
  }
}
